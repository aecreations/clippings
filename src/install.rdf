<?xml version="1.0"?>

<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">

  <Description about="urn:mozilla:install-manifest">
    <em:name>Clippings</em:name>
    <em:id>{91aa5abe-9de4-4347-b7b5-322c38dd9271}</em:id>
    <em:type>2</em:type>
    <em:version>5.5.3</em:version>
    <em:description>Save frequently-entered text for pasting later.</em:description>
    <em:creator>AE Creations</em:creator>
    <em:translator>Endor8 (de)</em:translator>
    <em:iconURL>chrome://clippings/skin/images/icon.png</em:iconURL>
    <em:homepageURL>http://aecreations.sourceforge.net/clippings/</em:homepageURL>
    <em:optionsURL>chrome://clippings/content/options.xul</em:optionsURL>

    <!-- Specify that the XPI must be unpacked into a directory.
         Required for packaged clippings to be located on first run. -->
    <em:unpack>true</em:unpack>

    <!-- Firefox -->
    <em:targetApplication>
      <Description>
        <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
	<em:minVersion>52.0</em:minVersion>
	<em:maxVersion>56.*</em:maxVersion>
      </Description>
    </em:targetApplication>
    <em:multiprocessCompatible>true</em:multiprocessCompatible>
    
    <!-- Thunderbird -->
    <em:targetApplication>
      <Description>
        <em:id>{3550f703-e582-4d05-9a08-453d09bdfdc6}</em:id>
	<em:minVersion>52.0</em:minVersion>
	<em:maxVersion>56.*</em:maxVersion>
      </Description>
    </em:targetApplication>
  </Description>      
</RDF>
