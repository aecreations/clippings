{
    "manifest_version": 2,
    "name": "Clippings/wx",
    "description": "Save frequently-entered text for pasting later.",
    "version": "6.0pre1",
    "homepage_url": "http://aecreations.sourceforge.net/clippings/index.php",
    
    "applications": {
	"gecko": {
	    "strict_min_version": "54.0"
	}
    },

    "icons": {
        "48": "icons/clippings-wx.png"
    },

    "permissions": ["contextMenus", "storage"],
    
    "background": {
	"scripts": ["dexie.js", "background.js"]
    },
    
    "content_scripts": [
	{
	    "matches": ["<all_urls>"],
	    "all_frames": true,
	    "js": ["content.js"]
	}
    ],
    
    
      
}
